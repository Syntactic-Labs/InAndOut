@model IEnumerable<InAndOut.Models.Item>
<h1>Index - Items</h1>

<table class="table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
          {
               <tr>
                    <td>@item.Id</td>
                    <td>@item.Borrower</td>
               </tr>
          }
    </tbody>
</table>